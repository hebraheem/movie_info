ailMovie(id) {\n  //   const detailM = getMovie(id);\n  //   setMovie([detailM]);\n  // }\n\n  return (\n    <movieContext.Provider\n      value={{\n        currentUser,\n        login,\n        signup,\n        logout,\n        logInput,\n        setLogInput,\n        resetPassword,\n        updatePassword,\n        updateEmail,\n        // movies,\n        // setMovies,\n        //loading,\n        search,\n        setSearch,\n        // movie,\n        // setMovie,\n        // detailMovie,\n        input,\n        setInput,\n      }}\n    >\n      {!check && children}\n    </movieContext.Provider>\n  );\n};\n\nexport const useMovieConsumer = () => {\n  return useContext(movieContext);\n};\n","/Users/aple/Desktop/project/movie_info/src/App.js",[],"/Users/aple/Desktop/project/movie_info/src/firebase.js",[],["148","149"],"/Users/aple/Desktop/project/movie_info/src/components/Home.js",["150"],"import { Button, makeStyles } from \"@material-ui/core\";\nimport React from \"react\";\nimport { ExitToApp, Telegram } from \"@material-ui/icons\";\nimport download from '../img/download.jpeg'\nimport {Link} from \"react-router-dom\"\nimport { useMovieConsumer } from \"../context\";\n\nconst useStyles = makeStyles(() => ({\n  button: {\n    backgroundImage: `url(${download})`,\n    backgroundRepeat: \"no-repeat\",\n    backgroundSize: \"cover\",\n    width: \"100%\",\n    height: \"90.8vh\",\n    display: \"flex\",\n    justifyContent: \"center\",\n    alignItems: \"center\",\n  },\n  link: {\n    textDecoration: \"none\",\n    color: \"white\"\n  }\n}));\n\nconst Home = () => {\n  const classes = useStyles();\n  const {data} = useMovieConsumer()\n\n  return (\n    <div className={classes.button}>\n      <Button\n        variant=\"contained\"\n        color=\"primary\"\n        size=\"large\"\n        startIcon={<Telegram />}\n        style={{ marginRight: \"20px\" }}\n      >\n        <Link className={classes.link} to=\"/signup\">\n          Sing Up\n        </Link>\n      </